<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='web3d'>

  <inherits name="elemental2.webgl.WebGl"/>
  <inherits name='org.geogebra.web.WebND'/>
    
  <!-- Specify the app entry point class.                         -->
  <entry-point class='org.geogebra.web.geogebra3D.Web3D'/>
  
  <!-- Specify the paths for translatable code                    -->
  <source path='geogebra3D'/>

  <set-configuration-property name="js.identifier.blacklist" value="Ink" />
  <set-configuration-property name="material_fetcher_path" value="js/materialFetcher.js" />

  <!-- Compile only one permutation -->
  <collapse-all-properties />
</module>
